<%- include ('./partials/header.ejs'); %> 
<body>
    <%- include ('./partials/navbar.ejs'); %> 
    <div class="container">
        <h1>Perguntas</h1>
        <!-- // Percorrer array -->
        <% perguntas.forEach(pergunta => { %>

            <div class="card mt-5">
                <div class="card-body">
                    <h3><p><%= pergunta.titulo %></p></h3>
                </div>
                <div class="card-footer">
                    <button class="btn btn-primary">Responder</button>
                </div>
            </div>
        <% }) %>
    </div>
    
</body>
<%- include ('./partials/footer.ejs'); %> 


